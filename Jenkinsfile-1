pipeline {
    agent any
    stages {
        stage('Performance Testing') {
            steps {
                echo 'Installing k6'
                sh 'echo "Sonatel2023" | sudo -S chmod +x setup_k6.sh'
                sh 'echo "Sonatel2023" |sudo ./setup_k6.sh'
                echo 'Running K6 performance tests...'
                sh 'k6 run test.js'
            }
        }
    }
}
